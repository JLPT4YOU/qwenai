{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api_server.py"
    },
    {
      "src": "/health",
      "dest": "/api_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api_server.py"
    }
  ]
}
